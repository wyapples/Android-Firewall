<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
	android:id="@+id/relativeLayout1"
	android:layout_width="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="wrap_content"
	android:gravity="fill_vertical">
	<TextView
		android:text="Information"
		android:layout_height="wrap_content"
		android:id="@+id/call_item_extra"
		android:layout_alignParentLeft="true"
		android:layout_width="wrap_content"
		android:textSize="26sp"></TextView>
	<TextView
		android:layout_below="@+id/call_item_extra"
		android:layout_width="wrap_content"
		android:id="@+id/call_item_numb"
		android:layout_height="wrap_content"
		android:text="TextView"
		android:layout_alignLeft="@+id/call_item_extra"></TextView>
	<TextView
		android:layout_width="wrap_content"
		android:id="@+id/call_item_date"
		android:layout_height="wrap_content"
		android:text="TextView"
		android:layout_alignTop="@+id/call_item_numb"
		android:layout_alignBottom="@+id/call_item_numb"
		android:layout_alignParentRight="true"></TextView>

</RelativeLayout>
